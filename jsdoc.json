{
	"tags": {
	  "allowUnknownTags": true
	},
	"source": {
	  "include": [
		"src/js/lib/modules/",
		"src/js/lib/components/",
		"src/js/lib/services/",
		"src/js/lib/core.js",
		"src/js/lib/lib.js"
	  ],
	  "includePattern": ".+\\.js(doc|x)?$",
	  "excludePattern": "(^|\\/|\\\\)_"
	},
	"opts": {
	  "destination": "./docs",
	  "recurse": true,
	  "template": "./node_modules/minami"
	}
  }